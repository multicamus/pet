<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">

		<!-- 프라이머리키 항상있어야 하고 중복되면 안되는걸 주의 -->
		<!-- namespace는 패키지명,파라미터타입은 메인바티스.xml에서 -->
		<!-- review_seq 라는 nextval 시퀀스를 디비에 만듦(리뷰넘버에 자동으로 번호를 늘리기위해서) -->
		<!-- 	SELECT review_seq.NEXTVAL FROM DUAL; 임의로 디비에 번호추가 >> review_seq.nextval는 1부터시작하니까 3까지 들어있다면 dual로 3까지 만들어줘야   -->
		<!--    #{review}는 dto로 감싸진 내가 입력한 데이터를 넣음  -->
<mapper namespace="multi.com.review">
	<insert id="review_id" parameterType="review_alias">
		insert into review
		(review_no,review)values(review_seq.nextval,#{review})
	</insert>
</mapper>